

{
  "name": "data-nord-fron",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "main.js",
  "build": {
    "appId": "com.yourcompany.yourapp",
    "productName": "Posttjenester Ã…ge-Haverstad",
    "files": [
      "dist/**/*",
      "main.js",
      "server.js",
      "src/scripts/*.js",
      "node_modules/**/*",
      "package.json"
    ],
    "asar": true,
    "asarUnpack": [
      "server.js",
      "src/scripts/*.js",
      "dist/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "node_modules",
        "to": "node_modules"
      }
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": ["dmg", "zip"]
    },
    "nsis": {
      "runAfterFinish": false
    }
  },
  "scripts": {
    "start-electron": "electron .",
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "postinstall": "electron-builder install-app-deps",
    "dist": "npm run build && electron-builder"
  },
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "emailjs-com": "^3.2.0",
    "express": "^4.21.2",
    "googleapis": "^148.0.0",
    "nodemailer": "^6.10.0",
    "process": "^0.11.10",
    "puppeteer": "^24.4.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
